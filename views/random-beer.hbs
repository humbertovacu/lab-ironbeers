<h1>Random Beer</h1>
<section>
    {{#each newBeer}}
        <img src="{{image_url}}" alt="">
        <h1>{{name}}</h1>
        <h2>{{tagline}}</h2>
        <p>{{description}}</p>
        <ul>Food Pairing:</ul>
        {{#each food_pairing}}
            <li>{{this}}</li>
        {{/each}}
        <p>Brewer tips: {{brewers_tips}}</p>
    {{/each}}
</section>