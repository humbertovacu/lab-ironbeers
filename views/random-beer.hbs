<div class = "individual-product">
    <div class = "title-container">
        <h1 class = "main-header">Random Beer</h1>
    </div>
        {{#each newBeer}}
            {{> beerpartial }}
            <div class = "food-pairing-container">
                <ul><h2>Food Pairing:</h2></ul>
                {{#each food_pairing}}
                    <li>{{this}}</li>
                {{/each}}
            </div>
            <h2>Brewer tips: </h2>
            <p>{{brewers_tips}}</p>
    {{/each}}
</div>